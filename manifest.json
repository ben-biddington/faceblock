{
    "description": "Block images of people with punchable faces",
    "manifest_version": 2,
    "name": "Faceblock",
    "version": "1.0",
    "homepage_url": "http://www.google.com",
    "icons": {
      "48": "icons/rose-diamond.ico"
    },
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["build/dist/faceblock.core.bundle.js", "faceblock.js"]
      }
    ],

    "browser_action": {
      "default_icon": {
        "19": "icons/rose-diamond.ico",
        "38": "icons/rose-diamond.ico"
      },
      "default_title": "",
      "default_popup": "adapters/web/popup/info.html"
    },

    "permissions": [
      "activeTab"
    ]
  }
  